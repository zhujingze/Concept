import json

# 读取原始 JSON 文件
with open('your_file.json', 'r') as file:
    data = json.load(file)

# 提取仅包含 key 为 "logits" 的部分
if "logits" in data:
    result = {"logits": data["logits"]}
else:
    result = {}

# 将结果保存为新的 JSON 文件
with open('filtered_file.json', 'w') as outfile:
    json.dump(result, outfile, indent=4)

print("Filtered JSON saved.")
